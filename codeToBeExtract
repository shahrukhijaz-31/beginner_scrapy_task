    <script type="text/javascript">
        general.iWishlistCount = 0;
        window.general.oUser.sToken = "2D44F0EE";
        window.general.oUser.sCSRFToken = "995ef2b0e7175324821db9c5dd0fb36b";

        window.general.initSession = function() {

            var xhttp = new XMLHttpRequest();

            xhttp.onreadystatechange = function () {
                if (xhttp.readyState === 4) {
                    if (xhttp.status === 200) {
                        var sessionData = JSON.parse(this.responseText.trim());
                        if (window.general.oUser && sessionData && sessionData.sToken && sessionData.sCSRFToken) {
                            window.general.oUser.sToken = sessionData.sToken;
                            window.general.oUser.sCSRFToken = sessionData.sCSRFToken;
                            if (sessionData.sUserProfileId){
                                window.gaDataLayer.push({
                                    'userProfileId' : sessionData.sUserProfileId
                                });
                            }
                        }

                    } else {
                        oTracking.pushEvent('Tech. Info', 'init-session', 'response status code = ' + xhttp.status);
                    }
                }
            };

            xhttp.onerror = function () {
                oTracking.pushEvent('Tech. Info', 'init-session', 'response status test = ' + xhttp.statusText);
            };

            xhttp.open("GET",'/index.php?cl=initSession', true);
            xhttp.send();
        };

        
    </script>



