        (function sh_af_browser(aBrowsers, aOSs) {
        var _sClasses = '',
                sWinNavi = window.navigator.userAgent.toLowerCase();
        if ( (/msie/).test(sWinNavi)  ) {
            _sClasses = _sClasses + ' msie';
            aBrowsers.msie = true;
            if ((/msie 9/).test(sWinNavi)) {
                _sClasses = _sClasses + ' ie9';
                aBrowsers.ie9 = true;
            } else if ((/msie 10/).test(sWinNavi)) {
                _sClasses = _sClasses + ' ie10';
                aBrowsers.ie10 = true;
            }
        } else if ((/trident\/7\./).test(sWinNavi) ) {
            _sClasses = _sClasses + ' ie11';
            aBrowsers.msie = true;
            aBrowsers.ie11 = true;
        } else if ( (/edge\//).test(sWinNavi)  ) {
            _sClasses = _sClasses + ' edge';
            aBrowsers.edge = true;
        } else if ( (/opera/).test(sWinNavi)  ) {
            _sClasses = _sClasses + ' opera';
            aBrowsers.opera = true;
        } else if ( (/webkit/).test(sWinNavi)  ) {
            _sClasses = _sClasses + ' webkit';
            aBrowsers.webkit = true;
            if ( (/chromium/).test(sWinNavi)  ) {
                _sClasses = _sClasses + ' chrome chromium';
                aBrowsers.chromium = true;
            } else if ( (/chrome/).test(sWinNavi)  ) {
                _sClasses = _sClasses + ' chrome';
                aBrowsers.chrome = true;
            } else if ( (/safari/).test(sWinNavi)  ) {
                _sClasses = _sClasses + ' safari';
                aBrowsers.safari = true;
            }
        } else if ( (/firefox/).test(sWinNavi)  ) {
            _sClasses = _sClasses + ' firefox';
            aBrowsers.firefox = true;
        }

        if ( (/windows/).test(sWinNavi) ) {
            _sClasses = _sClasses + ' windows-os';
            aOSs.windows = true;
        } else if ( (/iphone|ipad|ipod|ios/).test(sWinNavi)) {
            _sClasses = _sClasses + ' ios-os';
            aOSs.ios = true;
        } else if ((/android/).test(sWinNavi)) {
            _sClasses = _sClasses + ' android-os';
            aOSs.android = true;
        } else if ( (/mac/).test(sWinNavi)) {
            _sClasses = _sClasses + ' mac-os';
            aOSs.mac = true;
        }
        general.htmlClasses = _sClasses;
    }(general.browser.names, general.os.names));


    
    (function(document, general){
                if (
            (('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) &&
            !(general.browser.names.chrome && ((general.os.names.windows && navigator.maxTouchPoints === 0) || general.os.names.mac))
            ){
                general.htmlClasses += ' touch';
                general.browser.support.touch = true;
        } else {
            general.htmlClasses += ' no-touch';
        }
    })(document, general);
    var noop = (function() {
            function noop () {}
            noop.chain = function(){return this;};
            return noop;
        })(),
        element = function(element) {
            var type = typeof element;
            switch (type) {
                case "element":
                    return element;
                case "object":
                    if (element instanceof jQuery) {
                        if (element.length === 0) {
                            // Kein valides Element
                            break;
                        }
                        return element.get(0);
                    } else if (element instanceof Window || element instanceof Element){
                        return element;
                    }
                    break;
                case "string":
                    var el = document.querySelector(element);
                    return el;
            }
        },
        initFunction = {},
        shGetURLParameter = function(_sParam, _sUrl) {
            var _sUrl = _sUrl || window.location.search,
                sPageURL = _sUrl.replace('?', '');
            var sURLVariables = sPageURL.split('&');
            for (var i = 0; i < sURLVariables.length; i++)
            {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == _sParam)
                {
                    return sParameterName[1];
                }
            }
        },

        shSendEconda = (function(aEcondaPool){

            return function(sType, sArg, sContent){
                if(typeof econdaTracking === "object"){
                    if (arguments.length){
                        switch(sType){
                            case 'marker':
                                econdaTracking.sendEcondaMarker(sArg, sContent);
                                break;
                            case 'template':
                                econdaTracking.sendEcondaTemplate(sArg, sContent);
                                break;
                            case 'content':
                                econdaTracking.sendEcondaContent(sContent);
                                break;
                            case 'target':
                            default:
                                econdaTracking.sendEcondaTarget(sArg, sContent);
                                break;
                        }
                    } else {
                        for(var i = 0, cEvent, gaEventsLength = aEcondaPool.length; i < gaEventsLength; i++){
                            cEvent = aEcondaPool[i];
                            shSendEconda(cEvent.sType, cEvent.sArg, cEvent.sContent)
                        }
                        aEcondaPool = new Array();
                    }

                } else{
                    aEcondaPool.push({sType : sType, sArg: sArg, sContent: sContent})
                }
            }
        })(window.general.econda.events),

        debugMode = shGetURLParameter('debugMode') || false,
        currentInstance = null;

            currentInstance = 'production';
            
            disableJSSpeedtest = false;
    

        (function(){
        var _bDone,
            trackJavaScriptError = function(e) {
                        if (_bDone) return;

            var _sendEvent = function () {
                                var action,
                        errorObj = e.error,
                        errorMessage = (typeof errorObj !== "undefined") ? errorObj.message : e.message,
                        node,
                                                errorTime = general.browser.support.timingAPI ? (Math.round((new Date().getTime() - window.performance.timing.responseEnd) / 1000 * 10) / 10).toString() : 'unknown';
                if (typeof errorMessage !== "undefined") {
                    action = errorMessage;
                    if (typeof e.filename !== "undefined"){
                        action = action + '|file=' + e.filename +'|';
                    }
                    if (typeof e.lineno !== "undefined"){
                        action = action  + e.lineno;
                    }
                    if (typeof e.colno !== "undefined"){
                        action = action + ':' + e.colno;
                    }
                } else {
                    action = 'unknown error object';
                }
                                window.oTracking.pushEvent('JS Error', action, errorTime, 0);

                /* Container für pingdom. */
                node = document.createElement('div');
                node.setAttribute('class', 'js-testtool-jserror l-hidden');
                node.textContent = action;
                document.body.appendChild(node);
                 /* /Container für pingdom. */

                _bDone = true;
            };

            window.oTracking.data.jserror = true;
            _sendEvent();
        };

        if (window.addEventListener) {
            window.addEventListener('error', trackJavaScriptError, false);
        }
    })();
    
                var econdaActive = true;
                var econdaDebug = false;
            window.emosTrackVersion = 2;
    
