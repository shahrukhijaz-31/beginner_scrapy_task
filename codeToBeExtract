        pushEvent : (function(){
            var _aTemp, _aBuffer = [], _bEvent;
            return function(_sCategory, _sAction, _sLabel , _nValue, _bNonInteraction, _sCustomEventName){
                _nValue = typeof _nValue === 'number' ? _nValue : 0;
                _bNonInteraction = !(_bNonInteraction === false || _bNonInteraction === 'false');
                _aTemp = document.readyState === 'loading'
                        ?
                        (oTracking.data.jserror ? window.gaDataLayer : _aBuffer)
                        :
                        window.gaDataLayer;
                _aTemp.push({
                    'event' : _sCustomEventName ? _sCustomEventName : 'gtmEvent',
                    'eventCategory' : _sCategory,
                    'eventAction' : _sAction,
                    'eventLabel' : _sLabel,
                    'eventValue' : _nValue,
                    'eventNonInteraction' : _bNonInteraction
                });
                if (!_bEvent && _aBuffer.length){
                    _bEvent = true;
                    document.onreadystatechange = function () {
                        if (document.readyState === 'interactive'){
                            window.oTracking.pushBufferdEvents(_aBuffer);
                        }
                    };
                }
                debugMode && console.log('track event : ' + _sCategory + ' | ' + _sAction + ' | ' + _sLabel + ' | ' + _nValue + ' | ' + _bNonInteraction)
